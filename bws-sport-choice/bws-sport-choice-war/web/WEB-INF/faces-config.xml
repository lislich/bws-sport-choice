<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    
    <navigation-rule>
      <from-view-id>login.xhtml</from-view-id>
      <navigation-case>
        <from-outcome>login</from-outcome>
        <to-view-id>/secured/wahl.xhtml</to-view-id>
      </navigation-case>
      <navigation-case>
        <from-outcome>faq</from-outcome>
        <to-view-id>faq.xhtml</to-view-id>
      </navigation-case>
      <navigation-case>
        <from-outcome>kontakt</from-outcome>
        <to-view-id>kontakt.xhtml</to-view-id>
      </navigation-case>
      <navigation-case>
        <from-outcome>kursAnlegen</from-outcome>
        <to-view-id>secured/kursAnlegen.xhtml</to-view-id>
      </navigation-case>    
    </navigation-rule>
    
    <navigation-rule>
      <from-view-id>/secured/wahl.xhtml</from-view-id>
      <navigation-case>
          <from-outcome>faq</from-outcome>
          <to-view-id>faq.xhtml</to-view-id>
      </navigation-case>   
      <navigation-case>
        <from-outcome>kontakt</from-outcome>
        <to-view-id>kontakt.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
      <from-view-id>/secured/kursAnlegen.xhtml</from-view-id>
      <navigation-case>
          <from-outcome>faq</from-outcome>
          <to-view-id>faq.xhtml</to-view-id>
      </navigation-case>   
      <navigation-case>
        <from-outcome>kontakt</from-outcome>
        <to-view-id>kontakt.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    
</faces-config>
